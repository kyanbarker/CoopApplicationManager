{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./src"],
    "paths": {
      "@notionhq/client": ["./node_modules/@notionhq/client/build/src/index"]
    }

  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
